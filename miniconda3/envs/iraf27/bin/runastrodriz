# EASY-INSTALL-ENTRY-SCRIPT: 'drizzlepac==2.2.6','console_scripts','runastrodriz'
__requires__ = 'drizzlepac==2.2.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('drizzlepac==2.2.6', 'console_scripts', 'runastrodriz')()
    )
