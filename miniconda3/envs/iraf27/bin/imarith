if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('astroimtools==0.2.dev177', 'console_scripts', 'imarith')()
    )
