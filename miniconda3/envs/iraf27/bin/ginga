#!/fs/computo52/other0/dani/Omaira/CanariCam/CCAM/miniconda3/envs/iraf27/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ginga==2.7.2','console_scripts','ginga'
__requires__ = 'ginga==2.7.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ginga==2.7.2', 'console_scripts', 'ginga')()
    )
