#!/fs/computo52/other0/dani/Omaira/CanariCam/CCAM/miniconda3/envs/iraf27/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'cubeviz==0.0.2','gui_scripts','cubeviz'
__requires__ = 'cubeviz==0.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cubeviz==0.0.2', 'gui_scripts', 'cubeviz')()
    )
