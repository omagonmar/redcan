/logoACKN { 
  save 
  0.5 setlinewidth 
  90.8 25 moveto 
  /Times-Roman findfont 
  8.787 scalefont setfont 
  (Operated by the Association of Universities for ) show
  (Research in Astronomy, Inc., for the National ) show
  (Aeronautics and Space Administration) show restore
} def

/logoFIGURE {

  newpath 
  3.5 46.5 moveto 
  1.5 46.5 lineto
  1.5 1.5 lineto
  46.5 1.5 lineto
  46.5 46.5 lineto
  44.5 46.5 lineto stroke		% box around telescope

  newpath
  24 46.5 20.5 0 360 arc stroke		% entrance pupil

  newpath
  24 46.5 9 0 360 arc stroke		% secondary

  newpath				% compute angle of earth/box
  10.5 23 dup mul 10.5 dup mul sub sqrt	%  intersection, and draw arc
  atan /earthang exch def 
  24 -9 23 earthang 180 earthang sub arc 
  stroke

  newpath 				% left half of telescope tube
  24 16.95 moveto
  18.3 dup mul 9.25 dup mul sub sqrt 9.25
  atan /sideangle exch def
  24 28.2 11.25 270 dup sideangle sub arcn
  24 46.5 20.5 270 sideangle sub dup arcn
  stroke

  newpath				% right half of telescope tube
  24 16.95 moveto
  24 28.2 11.25 270 dup sideangle add arc
  24 46.5 20.5 270 sideangle add dup arc
  stroke

  newpath
  24 28.2 11.25 67 6 arcn		% upper right of primary and
  24 46.5 9 2.25 18.3 dup mul 2.25 dup mul sub sqrt atan dup arcn 	
  stroke					
					%  tangent to secondary
  newpath				% upper left of primary and
  24 28.2 11.25 113 174 arc		%  tangent to secondary
  24 46.5 9 180 2.25 18.3 dup mul 2.25 dup mul sub sqrt atan sub dup arc
  stroke

  newpath
  32.79 44.54 moveto	
  24 5.6 lineto				% tangent from secondary to earth
  15.21 44.54 lineto stroke 		% back to secondary
  newpath

} def

