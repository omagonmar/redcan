xc -h  fixfits.f libcfitsio32.a -o fixfits.e -limfort -lsys -lvops -los
fixfits.f:
   MAIN:
   writeimage:
   fixme:
   printerror:
   deletefile:
link:
xc -h  calc_off.f -o calc_off.e -limfort -lsys -lvops -los
calc_off.f:
   MAIN calc_off:
link:
xc -h  fixbad.f -o fixbad.e -limfort -lsys -lvops -los
fixbad.f:
   MAIN:
   fixme:
link:
move `fixfits.e' to `cirred$bin/fixfits.e'
move `fixbad.e' to `cirred$bin/fixbad.e'
move `calc_off.e' to `cirred$bin/calc_off.e'
