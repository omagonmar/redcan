# Make the imcalc control package.
 
update:

$ifnewer (imcalc.yy, imcalc.x)
        $ifeq (HOSTID, unix)
            $echo "imcalc.x is out of date; rebuilding with XYACC:"
            !(xyacc -v imcalc.yy; mv -f ytab.x imcalc.x)
        $else
            $echo "imcalc.x is out of date; rebuild with XYACC"
        $endif
$endif

$checkout libcalc.a ../
$update libcalc.a
$checkin libcalc.a ../
$exit
 
libcalc.a:
	imacts.x	imcalc.h imcalc.com
	imacts1.x	imcalc.h imcalc.com
	imcpu.x		imcalc.h imcalc.com
	imopk.x		imcalc.h imcalc.com
	xarithop.x	imcalc.h imcalc.com
	xboolop.x	imcalc.h imcalc.com
	xbranch.x	imcalc.h imcalc.com
	xcht.x		imcalc.h imcalc.com
	xcomplex.x	imcalc.h imcalc.com
	xfunc.x		imcalc.h imcalc.com
	xload.x		imcalc.h imcalc.com
	xlogicop.x	imcalc.h imcalc.com
	xproj.x		imcalc.h imcalc.com
	xprn.x		imcalc.h imcalc.com
	xsel.x		imcalc.h imcalc.com
	xstore.x	imcalc.h imcalc.com
	imcalc.x	imcalc.h imcalc.com lex.com errfcn.com
	errfcn.x	errfcn.com
        ;
