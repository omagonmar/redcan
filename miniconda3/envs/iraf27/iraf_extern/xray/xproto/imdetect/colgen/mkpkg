# Make more Image collapse library.

update:
$checkout libcolgen.a ../..
$update   libcolgen.a
$checkin  libcolgen.a ../..
$exit
 
tfiles:
	$set	GA   = "$$generic -k -p gen/"

	# Each of the following generic files is expanded for each of the
	# datatypes listed in the -t flag.

	$ifolder (gen/collapses.x, collapse.gx) $(GA) -t silrdx collapse.gx $endif
	;

libcolgen.a:
	$ifeq (USE_GENERIC, yes) $call tfiles $endif
	$set XFLAGS = "$(XVFLAGS)"
	@gen
	;
