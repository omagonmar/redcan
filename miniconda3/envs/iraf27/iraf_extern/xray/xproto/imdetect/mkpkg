update:
$checkout libcolgen.a ../
$update	libcolgen.a
$checkin libcolgen.a ../

$checkout libdet.a ../
$update	libdet.a
$checkin libdet.a ../

$exit

libcolgen.a:
	$call update@colgen
	;

