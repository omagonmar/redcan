# Make the scan library

update:
$checkout libscan.a ..
$update   libscan.a
$checkin  libscan.a ..
$exit

libscan.a:
	slcall.x <slset.h> <imhdr.h>
	slopen.x <slset.h>
	scadd.x <scset.h>
	sccircle.x <scset.h>
	scdebug.x <scset.h>
	scdisp.x <scset.h>
	scget.x <scset.h> <plset.h>
	scmerge.x <scset.h>
	scop.x <scset.h>
	scopen.x <scset.h>
	scpaint.x <scset.h>
	scplio.x <scset.h> <pmset.h>
	scpoint.x <scset.h>
	scpool.x <scset.h>
	scrotbox.x <scset.h>
	;



test:
	$omake	sctest.x scset.h
        $link	sctest.o ../libscan.a
	;

try:
	$omake	sltry.x scset.h
        $link	sltry.o ../libscan.a ../libregions.a
	;
