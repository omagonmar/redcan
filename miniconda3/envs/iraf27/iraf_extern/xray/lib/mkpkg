$ifeq (hostid, unix) !(clear;date) $endif
#$checkout libpros.a xraybin$
#$checkout libscan.a xraybin$
#$checkout libfft.a xraybin$
$call update
#$checkin libpros.a xraybin$
#$checkin libscan.a xraybin$
#$checkin libfft.a xraybin$
$exit

update:
	$echo "-------------- LIB/COORDS ----------------"
	@coords
	$echo "-------------- LIB/PROS ----------------"
	@pros
	$echo "-------------- LIB/QPCREATE ----------------"
	@qpcreate
	$echo "-------------- LIB/REGIONS ----------------"
	@regions
	$echo "-------------- LIB/SCAN ----------------"
	@scan
	$echo "-------------- LIB/FFT ----------------"
	@fft
	;


