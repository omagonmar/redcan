# Make control package.
 
update:

$ifnewer (pixparse.yy, pixparse.x)
        $ifeq (HOSTID, unix)
            $echo "pixparse.x is out of date; rebuilding with XYACC:"
            !(xyacc -v pixparse.yy; mv -f ytab.x pixparse.x)
        $else
            $echo "pixparse.x is out of date; rebuild with XYACC"
        $endif
$endif


$checkout libmodel.a ../
$update libmodel.a
$checkin libmodel.a ../


libmodel.a:
	immodel.x
	imsmooth.x
	pixparse.x pixparse.h pixparse.com pixlex.com
	pixacts.x pixparse.h pixparse.com
	;	
	
$exit
