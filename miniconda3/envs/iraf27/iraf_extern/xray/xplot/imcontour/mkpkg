# Make control package.
 
$ifnewer (clevparse.yy, clevparse.x)
     $ifeq (HOSTID, unix)
       $echo "clevparse.x is out of date; rebuilding with XYACC:"
       !(rm -f clevparse.x; xyacc clevparse.yy;sed "s/yyparse/clev_yyparse/g" < ytab.x > clevparse.x;rm -f ytab.x)
     $else
       $echo "clevparse.x is out of date; rebuild with XYACC"
     $endif
$endif

$checkout libpkg.a ..
$update libpkg.a
$checkin libpkg.a ..
$exit
 

libpkg.a:
	imcontour.x <gset.h> <imhdr.h> <ext.h> <mach.h> imcontour.h clevels.h
	clevparse.x <error.h> <ctype.h> <lexnum.h> <finfo.h> clevparse.com clevparse.h
	clevstore.x clevels.h clevels.com
	imc_clevels.x <imhdr.h> <math.h> <mach.h> clevels.h imcontour.h
	imc_contours.x <gset.h> imcontour.h clevels.h clevels.com
	imc_pixgrid.x <gset.h> imcontour.h
	imc_rddata.x <imset.h> <imhdr.h> <tbset.h> <error.h> <gset.h> imcontour.h <precess.h> <math.h>
	imc_skymap.x <gset.h> <math.h> <mach.h> imcontour.h clevels.h
	imc_legend.x <gset.h> <math.h> imcontour.h clevels.h clevels.com
	imc_utils.x <gset.h> <math.h> <imhdr.h> <qpoe.h> <mach.h> imcontour.h clevels.h
        imc_subs.x <math.h> <mach.h> imcontour.h 
        ;
