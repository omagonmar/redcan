# Make the xdataio package.

 
$checkout libpkg.a ../
$update libpkg.a
$checkin libpkg.a ../
$exit
 
libpkg.a:
	$ifnewer (datapar.y, datapar.x)
	    $ifeq (HOSTID, unix)
		$echo "datapar.x is out of date; rebuilding with XYACC:"
		!(rm -f datapar.x; xyacc datapar.y; mv ytab.x datapar.x)        	    $else
		$echo "datapar.x is out of date; rebuild with XYACC"
	    $endif
	$endif
	datarep.x	datarep.h
	datapar.x       datarep.h datapar.com
	datarun.x       datarep.h datarun.com
	iee2host.x
	vax2host.x
	dg2host.x
	vax2iee.x
	host2txt.x	datapar.com hst2txt.com
	;
