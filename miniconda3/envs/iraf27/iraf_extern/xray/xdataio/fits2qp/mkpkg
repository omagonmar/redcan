# Make the xdataio package.

 
$checkout libpkg.a ../
$update libpkg.a
$checkin libpkg.a ../
$checkout libimpkg.a ../
$update libimpkg.a
$checkin libimpkg.a ../
$exit
 
libpkg.a:
        fits2qp.x 	<mach.h> <qpset.h> <ext.h> "cards.h" "ftwcs.h" "fits2qp.h" "fits2qp.com" "mpefits.h"

        ft_header.x     <mach.h> <evmacro.h> <wfits.h> "fits2qp.h" "cards.h" "fits2qp.com"

        ft_nxtext.x     <mach.h> <math.h> <evmacro.h> <wfits.h> "fits2qp.h" "cards.h" "ftwcs.h" "fits2qp.com" "mpefits.h"

	ft_enterwcs.x	<ctype.h> <imhdr.h> <imio.h> "ftwcs.h" "mwcs.h"
	ft_mwcs.x	<error.h> <imhdr.h> <imio.h> "ftwcs.h" "mwcs.h"
        knowncards.x    cards.h <ctype.h> cards.com
        nextcard.x      cards.h <ctype.h> cards.com
	ft_rename.x	"fits2qp.h"
	ft_typedef.x	<ctype.h> <evmacro.h> "fits2qp.h"
	ft_extno.x	<ctype.h>
	ft_addaux.x	<mach.h> <qpset.h> "fits2qp.h"
	ft_addev.x	<mach.h> <evmacro.h> "fits2qp.h" "ftwcs.h" "fits2qp.com" "mpefits.h" "cards.h"
	ft_savev.x	<evmacro.h> "fits2qp.h" "fits2qp.com" "cards.h"
	ft_skip.x	<mach.h> "fits2qp.h"
	ft_addparam.x	"cards.h"
	ft_duplparam.x	"cards.h"
	ft_standard.x	<qpset.h> <evmacro.h> "fits2qp.h" "fits2qp.com" "cards.h"
	ft_macros.x
	ft_hist.x
	ft_mpe_event.x  <ctype.h> <mach.h> <evmacro.h> <fset.h> "cards.h" "ftwcs.h" "fits2qp.h" "mpefits.h"
	ft_mpe_head.x   <ctype.h> <mach.h> <evmacro.h> <fset.h> <coords.h> <rosat.h> "ftwcs.h" "cards.h" "fits2qp.h" "mpefits.h"
	ft_mpe_utl.x    <ctype.h> <mach.h> <evmacro.h> <fset.h> "cards.h" "ftwcs.h" "fits2qp.h" "mpefits.h"
	ft_ratfits.x "fits2qp.h" "cards.h" "ftwcs.h"
	ft_mii.x	<mach.h> <mii.h> <iraf.h> <ctype.h> "fits2qp.h"
	;

libimpkg.a:
        fits_cards.x    wfits.com wfits.h <imhdr.h>
        fits_params.x   wfits.h <time.h>
        fits_read.x     rfits.com rfits.h <error.h> <fset.h> <imhdr.h>
        fits_rheader.x  rfits.com rfits.h rfits.com <ctype.h> <imhdr.h>\
                        <imio.h> <mach.h> "ftwcs.h"
        fits_rimage.x   rfits.com rfits.h <imhdr.h> <fset.h> <mach.h>
        fits_rpixels.x  <fset.h> <mach.h> <mii.h>
        t_rfits.x       rfits.com rfits.h <error.h> <fset.h>
        im_mpe_head.x   <ctype.h> <mach.h> <evmacro.h> <fset.h> <coords.h> \
                        <rosat.h> <clk.h> "cards.h" "ftwcs.h" "fits2qp.h" \
                        "mpefits.h" "rfits.h" "wfits.h"

	;
