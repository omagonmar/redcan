\T=xterm
\G=xtermr
clear\n
\{%5000 

	[An SAOimage window MUST be active for this demo]
\}
\{%5000

	[Change directory to output filespace]
\}\n
cd\sout_demo\n
\{%5000
	
	[Delete existing demo files]
\}\n
imdel\spabbell*.imh\n
del\s\s\spabbell*.pl\n
\{%5000

	[Load the "xray", "xplot", and "xspatial" packages]
\}\n
xray\n
xplot\n
xspatial\n
\{%5000

	[Copy a blocked sub-section of the input QPOE file 
	 into a temporary file]
\}\n
imcopy\s("in_demo$pabbell.qp[bl=10][501:1012,456:967]",\s"pabbell.imh",\n
verbose=yes)\n
\{%5000

	[Smooth the data]
\}\n
imsmooth("pabbell.imh[129:384,129:384]",\s".",\s"gauss",\s2.25,\s0)\n
\{%5000

	[Run the "tvimcontour" task]
\}\n
tvimcontour\s("pabbell_smo",\n
"sky",\s"peak",\s"log\s10\s80\s5",\s"",\sgcolor="w",\swlpars="",\sdisplay\
    =2,\n
dotitle=yes,\sdolegend=yes,\ssrc_list="none",\sracol="ra",\sdeccol="dec",\n
isystem="",\ssrc_mark="+#",\spixgrid="full",\spixlabels="in",\spixel_lines=\
    10,\n
dolabel=yes,\smajor_grid=yes,\sminor_grid=no,\szrange=yes,\sz1=0.,\sz2=30.,\n
ztrans="linear")\n
\{%5000

	[Run the "isoreg" task]
\}\n
isoreg\s("pabbell_smo.imh",\s"pabbell",\n
\s\s\s\s\s\s\s\s"0.5397,0.8181,1.2399,1.8794,2.8487,4.3178",\sclobber=no,\s\
    display=2)\n
\{%5000

	[Run the "imcnts" task]
\}\n
imcnts\s("pabbell_smo.imh",\n
"pabbell_iso.pl",\s"none",\s"",\s"none",\sexposure="NONE",\sexpthresh=0.,\n
err="NONE",\smatchbkgd=no,\sbkgdexposure="NONE",\sbkgdthresh=0.,\saddbkgder\
    r=yes,\n
bkgderr="NONE",\stimenorm=no,\snormfactor=1.,\sclobber=no,\sdisplay=1)\n
\{%5000

	[Exit the "xray", "xplot", and "xspatial" packages]
\}\n
bye\n
bye\n
bye\n
gflush\n
cd\sscripts_demo\n
stty\sclear\slogin\n
